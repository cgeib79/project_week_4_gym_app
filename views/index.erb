<h1>Member List gym_app</h1>
<div>
<div id="button"><a href="login.html">Click here to add new member</a></div>
</div>
  <hr/ >
<div class="grid">
  <% @members.each do |member| %>
  <p> <%= member.first_name() %> <%= member.second_name() %> </p>
  <p> <%= member.membership_tier %> </p>
  <p><a href="/members/<%= member.id %>">Show member</a></p>
  <hr />
  <% end %>
</div>
